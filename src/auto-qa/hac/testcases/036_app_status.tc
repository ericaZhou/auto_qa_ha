prepare clean env

enable app scripts

var master slave
var sip
var nic

get status

assert status has `
	+-- {master}({nic}) with SIP
	+-- HA Service: Running
	+-- MySQL Service: Running 
    +-- External App: started
`

assert status has `
	+-- {slave}({nic})
    +-- HA Service: Running
    +-- MySQL Service: Running
    +-- MySQL Replication: Running
    +-- External App: started
`

rollback:
disable app scripts